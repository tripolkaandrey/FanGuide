@model FanGuide.ViewModels.TeamDetailsViewModel
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>

<ul>
    <li>Name: @Model.Name</li>
    <li>Sport: @Model.SportId</li>
    @foreach (var athlete in Model.Athletes)
    {
        <li>@athlete</li>
    }
</ul>